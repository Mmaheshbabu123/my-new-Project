{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/Services/*": [
        "Services/*"
      ],
      "@/styles/*": [
        "styles/*"
      ],
      "@/Contexts/*": [
        "Contexts/*"
      ],
      "@/atoms/*": [
        "components/core-module/atoms/*"
      ],
      "@/Constants": [
        "Services/Constants"
      ],
    },
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": [
    "node_modules"
  ]
}
